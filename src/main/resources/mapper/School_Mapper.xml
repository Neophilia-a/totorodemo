<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.totoro.tororodemo.design_pattern.design_demo_15_02.dao.ISchoolDao">

    <select id="querySchoolInfoById" resultType="com.totoro.tororodemo.design_pattern.design_demo_15_02.po.School">
        SELECT id, name, address, createTime, updateTime
        FROM school
        where id = #{id}
    </select>

</mapper>